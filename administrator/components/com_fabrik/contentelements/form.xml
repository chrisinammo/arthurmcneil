<?xml version="1.0" ?>
<joomfish type="contentelement">
  <name>Fabrik - Forms</name>
  <author>Manel Zaera, manelzaera@yahoo.es</author>
  <version>1.0 for Fabrik 2.0</version>
  <description>Definition for Fabrik Form</description>
  <reference>
  	<table name="fabrik_forms">
  		<field type="referenceid" name="id" translate="0">ID</field>
  		<field type="titletext" name="intro" translate="1">Intro text</field>  		
  		<field type="text" name="label" translate="1">Title</field>  		
  		<field type="text" name="error" translate="1">Error message</field>  		  		
  		<field type="text" name="form_subject" translate="1">Form subject</field>
  		<field type="text" name="form_submit_message" translate="1">Form submit message</field>  		
  		<field type="text" name="receipt_message" translate="1">Form receipt e-mail text</field>  		
  		<field type="text" name="reset_button_label" translate="1">Reset button label</field>  		
  		<field type="text" name="submit_button_label" translate="1">Submit button label</field>  		
  		<field type="created_date" name="created" translate="0">Created</field>
  		<field type="modified_date" name="modified" translate="0">Modified</field>  		
  		<field type="checked_out_date" name="checked_out_time" translate="0">Check out date</field>
  	</table>
  </reference>
</joomfish >
